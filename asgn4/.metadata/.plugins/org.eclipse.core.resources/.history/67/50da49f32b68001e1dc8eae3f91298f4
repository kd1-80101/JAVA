package com.market.fruits;

public class Mango extends Fruit {
public Mango() {
	this("", 00.0,"", true);
}
	public Mango(String color, Double weight, String name, boolean fruitState) {
		super(color, weight, name, fruitState);
	}
@Override
public void acceptData() {
	super.acceptData();
}

@Override
public void displayData() {
	super.displayData();
	this.taste();
	
}
@Override
public String taste() {
	return "sweet taste ";
}

}
