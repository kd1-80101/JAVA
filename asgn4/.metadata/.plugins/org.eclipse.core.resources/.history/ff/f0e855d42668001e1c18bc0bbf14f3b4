package com.market.fruits;

public class Apple extends Fruit {

	public Apple() {
		// TODO Auto-generated constructor stub
	}
	public Apple(String color, Double weight, String name, boolean fruitState) {
		super(color, weight, name, fruitState);
	
	}
@Override
public void acceptData() {
	super.acceptData();
}

@Override
public void displayData() {
	super.displayData();
	this.taste();
	
}
@Override
public String taste() {
	return "sweet and sour taste ";
}
}
